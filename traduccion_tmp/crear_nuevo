xgettext -k_ -kN_ -o messages.pot ../meowndloads_app/*.py ../meowndloads_app/ui/interfaz.ui.h --from-code UTF-8

# Editar con POEDIT
msginit -i messages.pot -o pt.po 

# crear arcivo que es
msgfmt pt.po -o ../po/pt/LC_MESSAGES/window_multi.mo

#Unir una anterior con una nueva
msgmerge pt.po messages.pot -o pt.po

intltool-extract --type="gettext/glade" ../meowndloads_app/ui/interfaz.ui

